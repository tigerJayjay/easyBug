<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Maper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easybug.dao.UserDao">
    <select id="queryUser" resultType="com.easybug.model.User">
        select * from user
    </select>
    <insert id="insertUser" useGeneratedKeys="true" keyProperty="id" keyColumn="id" parameterType="com.easybug.model.User">
        insert into user(userName,password) values(#{userName},#{password})
    </insert>
</mapper>