<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Maper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.easybug.dao.TestDao">
    <select id="queryTest" resultType="com.easybug.entity.Test">
        select * from test1
    </select>
    <insert id="insertTest" useGeneratedKeys="true" keyProperty="id" keyColumn="id" parameterType="com.easybug.entity.Test">
        insert into test1(tid) values(#{tid})
    </insert>
</mapper>